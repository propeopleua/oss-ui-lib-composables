{
  "name": "oss-composable",
  "private": true,
  "version": "0.0.2",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "scripts": {
    "dev": "NODE_ENV=development vite build",
    "build": "NODE_ENV=production tsc && vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "last 5 versions"
  ],
  "files": [
    "*"
  ],
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.5"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.21"
  },
  "exports": {
    "./useList": {
      "import": "./src/list/useList.ts"
    },
    "./useForm": {
      "import": "./src/form/useForm.ts"
    },
    "./useTmplRefs": {
      "import": "./src/tmpl/useTmplRefs.ts"
    },
    "./utils/scrollTo": {
      "import": "./src/utils/scrollTo.ts"
    },
    "./utils/recordToFormData": {
      "import": "./src/utils/recordToFormData.ts"
    },
    "./useUpsideDown": {
      "import": "./src/upside-down/useUpsideDown.ts"
    }
  }
}
